# Environment variables for the domain service
SERVICE_APP_LOG_LEVEL=debug
SERVICE_APP_DICTIONARY_STUB_ENABLED=true
SERVICE_APP_GOOGLE_STUB_ENABLED=true
SERVICE_APP_OPENAI_STUB_ENABLED=true
SERVICE_APP_GRPC_PORT=8080
SERVICE_APP_MONGO_CARD_PROTOCOL=mongodb
SERVICE_APP_MONGO_CARD_HOST=mongo
SERVICE_APP_MONGO_CARD_URI_PARAMS="retryWrites:true,w:majority"
SERVICE_APP_MONGO_CARD_DATABASE=dictionary
SERVICE_APP_MONGO_CARD_COLLECTION=cards
SERVICE_APP_USER_SESSION_EXPIRATION=1h
SERVICE_APP_USER_SESSION_KEY_PREFIX=user-session

# Environment variables for the mongo db container
MONGO_DB_PORT=27017
MONGO_DB_USERNAME=root
MONGO_DB_PASSWORD=pass
